<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" 
				xmlns:s="library://ns.adobe.com/flex/spark" 
				xmlns:mx="library://ns.adobe.com/flex/mx" 
				autoDrawBackground="true">
	
	<s:HGroup>
		<s:Label text="{data.firstName + ' '}" color="#854300"  fontSize="14" fontWeight="bold"/>
		<s:Label text="{data.lastName}" color="#854300"  fontSize="14" fontWeight="bold"/>
		<s:Label text="{' (' + data.nickName + ') \n'}" color="#000000"  fontSize="14" />
	</s:HGroup>	
</s:ItemRenderer>
